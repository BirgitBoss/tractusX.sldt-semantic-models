@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.shared.quality_core:1.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#> .

:QualityCore a samm:Aspect ;
   samm:preferredName "Quality core"@en ;
   samm:description "This semantic model serves as library for other Catena-X models from Quality Management. It ensures that properties that are used over different models are defined in the same way."@en ;
   samm:properties ( :generalProperties :metaInformation :partProperties :vehicleProperties ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:generalProperties a samm:Property ;
   samm:preferredName "General properties"@en ;
   samm:description "Bundling general properties that do not belong to parts or vehicles."@en ;
   samm:characteristic :GeneralInformationCharacteristic .

:metaInformation a samm:Property ;
   samm:preferredName "Meta information"@en ;
   samm:description "This meta information is valid for all entries in this data set. The meta information is used to tell the data consumer which selection criteria were applied by the data provider. "@en ;
   samm:characteristic :MetaCharacteristic .

:partProperties a samm:Property ;
   samm:preferredName "Part Properties"@en ;
   samm:description "Bundling part/component properties. A part/component is meant as one piece in a vehicle. The terms part and component are synonymes."@en ;
   samm:characteristic :PartCharacteristic .

:vehicleProperties a samm:Property ;
   samm:preferredName "Vehicle properties"@en ;
   samm:description "Bundling vehicle/car properties like anonymizedVIN or mileage counter."@en ;
   samm:characteristic :VehicleCharacteristic .

:GeneralInformationCharacteristic a samm:Characteristic ;
   samm:preferredName "General information characteristic"@en ;
   samm:description "General information characteristic"@en ;
   samm:dataType :GeneralProperties .

:MetaCharacteristic a samm:Characteristic ;
   samm:preferredName "MetaCharacteristic"@en ;
   samm:description "Characteristic for the meta information."@en ;
   samm:dataType :MetaInformation .

:PartCharacteristic a samm:Characteristic ;
   samm:preferredName "Part characteristic"@en ;
   samm:description "Part characteristic"@en ;
   samm:dataType :PartProperties .

:VehicleCharacteristic a samm:Characteristic ;
   samm:preferredName "Vehicle characteristic"@en ;
   samm:description "Vehicle characteristic"@en ;
   samm:dataType :VehicleProperties .

:GeneralProperties a samm:Entity ;
   samm:preferredName "General properties"@en ;
   samm:description "Bundling general properties that do not belong to parts or vehicles."@en ;
   samm:properties ( :catenaXQualityTaskId :countryCode :hexValue :latitude :longitude :recordStatus :semanticModel :semanticModelVersion :sessionId :workShopId ) .

:MetaInformation a samm:Entity ;
   samm:preferredName "Meta Information"@en ;
   samm:description "This entity groups all meta information properties. Meta information properties give hints for a Catena-X data consumer to better understand, how this data set was created on the Catena-X data provider side."@en ;
   samm:properties ( :selectionCriteria [ samm:property :selectionStart; samm:optional true ] [ samm:property :selectionEnd; samm:optional true ] ) .

:PartProperties a samm:Entity ;
   samm:preferredName "Part Properties"@en ;
   samm:description "Bundling part/component properties. A part/component is meant as one piece in a vehicle. The terms part and component are synonyms. Some parts/components are produced/manufacturer by a supplier and sold by an automotive manufacturer/original equipment manufacturer (OEM). That's why for the same part different part numbers can exists: e.g. manufacturerPartNumber that is used within the manufacturing company and oemPartNumber that is labelled on the part itself. Spare parts from aftermarket have often only manufacturerPartNumber on them."@en ;
   samm:properties ( :catenaXPartId :dataMatrixCode :manufacturerPartName :manufacturerPartNumber :manufacturerSerialNumber :oemPartName :oemPartNumber :oemSerialNumber :supplierId ) .

:VehicleProperties a samm:Entity ;
   samm:preferredName "Vehicle properties"@en ;
   samm:description "Bundling vehicle/car properties like anonymizedVIN or mileage counter."@en ;
   samm:properties ( :anonymizedVIN :catenaXVehicleId :mileage ) .

:catenaXQualityTaskId a samm:Property ;
   samm:preferredName "Quality Task ID"@en ;
   samm:description "A unique quality task identifier within the Catena-X data-space. The unique id uses the Catena-X UUID trait."@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "430f56d3-1234-1234-1234-abc123456789" .

:countryCode a samm:Property ;
   samm:preferredName "Country code"@en ;
   samm:description "Country code in  ISO 3166-1 alpha-3 codes where this session took place"@en ;
   samm:characteristic :CountryCodeTrait ;
   samm:exampleValue "DEU" .

:hexValue a samm:Property ;
   samm:preferredName "Hex value"@en ;
   samm:description "Hex values use are encoded in  0-9,a-f or A-F to store information. "@en ;
   samm:characteristic :HexValueTrait ;
   samm:exampleValue "0x343343443af" .

:latitude a samm:Property ;
   samm:preferredName "Latitude"@en ;
   samm:description "Latitude property for GPS coordinates."@en ;
   samm:see <https://www.w3.org/2003/01/geo/#overview> ;
   samm:characteristic :LatitudeTrait ;
   samm:exampleValue "9.165877215355193"^^xsd:float .

:longitude a samm:Property ;
   samm:preferredName "Longitude"@en ;
   samm:description "Longitude property for GPS coordinates."@en ;
   samm:see <https://www.w3.org/2003/01/geo/#overview> ;
   samm:characteristic :LongitudeTrait ;
   samm:exampleValue "48.81109356017604"^^xsd:float .

:recordStatus a samm:Property ;
   samm:preferredName "Record status"@en ;
   samm:description "The record operation enumeration can be used to realize delta update concept.\nDelta update concept means:\n- You transfer an initial load of data\n- After the first week only the delta to the initial load is transferred\n\n\nThe record status describes whether this record is:\n- new=>This record is transferred the first time\n- update=> Some properties of this record have changed compared to a previous transfer\n- delete=> This record was transferred in the initial load or in a previous delta update, but is not used any more and therefore it should be deleted on data consumer side\n- same=> This record was transferred in the initial load or in a previous delta update"@en ;
   samm:characteristic :RecordStatusEnumeration ;
   samm:exampleValue "update" .

:semanticModel a samm:Property ;
   samm:preferredName "Semantic model"@en ;
   samm:description "Semantic model without version information. So basically everything before the version information.\n"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "io.catenax.shared.quality_core" .

:semanticModelVersion a samm:Property ;
   samm:preferredName "Semantic model version"@en ;
   samm:description "The version of a semantic model in the format x.y.z. X is the major version number, y is the minor version number and z is the patch number."@en ;
   samm:characteristic :ModelVersionTrait ;
   samm:exampleValue "1.0.0" .

:sessionId a samm:Property ;
   samm:preferredName "Session identifier"@en ;
   samm:description "An OEM-specific session identifier that is unique in this company."@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "3747429FGH382923974682_Session_2023-11-04T08:00:45" .

:workShopId a samm:Property ;
   samm:preferredName "Workshop ID"@en ;
   samm:description "OEM-specific identifier for a workshop."@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "workshop-4563328" .

:selectionCriteria a samm:Property ;
   samm:preferredName "Selection criteria"@en ;
   samm:description "Describes the selection criteria a Catena-X data provider has applied to create this data set."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Export of data that data that belongs to one or more Catena-X Quality tasks." .

:selectionStart a samm:Property ;
   samm:preferredName "Selection start"@en ;
   samm:description "Describes the starting point for providing records in this data set."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "2023-01-01T00:00:00" .

:selectionEnd a samm:Property ;
   samm:preferredName "Selection end"@en ;
   samm:description "Describes the end point for providing records in this data set."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "2023-12-31T23:59:59" .

:catenaXPartId a samm:Property ;
   samm:preferredName "Catena-X part ID"@en ;
   samm:description "Catena-X ID of one part. A Catena-X ID is unique in Catena-X data-space"@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "urn:uuid:580d3adf-1981-44a0-a214-13d6ceed9000" .

:dataMatrixCode a samm:Property ;
   samm:preferredName "Datamatrix code"@en ;
   samm:description "String that is behind a data matrix code. Data matrix codes or short DMC are labelled/printed on components/parts. Example: A data matrix code can combine supplier name, part type, plant who manufactured this part, assembly line and production date. This resulting string is normally unique."@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "3#5ZZ9454554CE#2024-07-10#BR11" .

:manufacturerPartName a samm:Property ;
   samm:preferredName "Manufacturer part name"@en ;
   samm:description "Manufacturer specific part name. A part name is not unique - same kinds of parts can share same OEM part name."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Gear box Gen9" .

:manufacturerPartNumber a samm:Property ;
   samm:preferredName "Manufacturer part number"@en ;
   samm:description "Part number given from  the manufacturer or supplier of this part, component, sub component or raw material. A manufacturer part number is NOT unique: All parts of the same kind share the same part number."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "FZ206460050202212" .

:manufacturerSerialNumber a samm:Property ;
   samm:preferredName "Manufacturer serial number"@en ;
   samm:description "Serial number given from the manufacturer or supplier of this part, component, sub component. A manufacturer serial number is unique: With the serial number the produced part can be traced within the manufacturer.\nManufacturer serial numbers are often use for spare parts from the aftermarket."@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "GBH3232434535454545-3534535" .

:oemPartName a samm:Property ;
   samm:preferredName "OEM part name"@en ;
   samm:description "OEM specific part name. A part name is not unique - same kinds of parts can share same OEM part name."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Gear box control unit." .

:oemPartNumber a samm:Property ;
   samm:preferredName "OEM Part number"@en ;
   samm:description "OEM specific part number. A part number is not unique - same kinds of parts can share same OEM part number."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "AD12345" .

:oemSerialNumber a samm:Property ;
   samm:preferredName "OEM Serial number"@en ;
   samm:description "Serial number as given from the OEM of one specific  part. A serial number is unique that means no other part/component has the same serial number."@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "ECU20646005020221" .

:supplierId a samm:Property ;
   samm:preferredName "SupplierID"@en ;
   samm:description "Specific ID of the supplier that manufactured the part that was put out - available if known"@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "ZF2064600502" .

:anonymizedVIN a samm:Property ;
   samm:preferredName "Anonymized VIN"@en ;
   samm:description "OEM-specific hashed VIN. Necessary to link current data set with other data sets."@en ;
   samm:characteristic :UniqueID ;
   samm:exampleValue "3747429FGH382923974682" .

:catenaXVehicleId a samm:Property ;
   samm:preferredName "Vehicle Catena-X Identifier"@en ;
   samm:description "A unique identifier for the vehicle in the Catena-X data space."@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "urn:uuid:580d3adf-1981-44a0-a214-13d6ceed9379" .

:mileage a samm:Property ;
   samm:preferredName "Mileage"@en ;
   samm:description "The mileage counter in the car. Only positive numbers are allowed."@en ;
   samm:characteristic :MileageCounter ;
   samm:exampleValue "23500"^^xsd:nonNegativeInteger .

:CountryCodeTrait a samm-c:Trait ;
   samm:preferredName "Country Code Definition"@en ;
   samm:description "This trait ensures a 3 characters country code."@en ;
   samm-c:baseCharacteristic :CountryCodeCharacteristic ;
   samm-c:constraint :CountryCodeRegularExpression .

:HexValueTrait a samm-c:Trait ;
   samm:preferredName "Hex Value Trait"@en ;
   samm:description "This trait allows 0-9, a-f, A-F in a string value. Optional prefix is \"0x\"."@en ;
   samm-c:baseCharacteristic :HexCodeCharacteristic ;
   samm-c:constraint :HexRegularExpression .

:LatitudeTrait a samm-c:Trait ;
   samm:preferredName "Latitude Trait"@en ;
   samm:description "Trait to define the latitude."@en ;
   samm-c:baseCharacteristic :Float ;
   samm-c:constraint :ConstraintLatitude .

:LongitudeTrait a samm-c:Trait ;
   samm:preferredName "Longitude Trait"@en ;
   samm:description "Trait to define the longitude."@en ;
   samm-c:baseCharacteristic :Float ;
   samm-c:constraint :ConstraintLongitude .

:RecordStatusEnumeration a samm-c:Enumeration ;
   samm:preferredName "Record status enumeration"@en ;
   samm:description "Defining the enumeration for the record status"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "new" "update" "delete" "same" ) .

:ModelVersionTrait a samm-c:Trait ;
   samm:preferredName "Model version trait"@en ;
   samm:description "Trait of the semantic model version."@en ;
   samm-c:baseCharacteristic samm-c:Text ;
   samm-c:constraint :SemanticModelExpression .

:UniqueID a samm:Characteristic ;
   samm:preferredName "unique ID"@en ;
   samm:description "This Characteristic is linked to a property that is unique. \nPlease see description of the linked property to get more details about uniqueness."@en ;
   samm:dataType xsd:string .

:MileageCounter a samm-c:Measurement ;
   samm:preferredName "Mileage"@en ;
   samm:description "The mileage counter of the car."@en ;
   samm:dataType xsd:nonNegativeInteger ;
   samm-c:unit unit:kilometre .

:CountryCodeCharacteristic a samm:Characteristic ;
   samm:preferredName "Country Code Characteristic"@en ;
   samm:description "ISO 3166-1 alpha-3 ? three-letter country codes "@en ;
   samm:dataType xsd:string .

:CountryCodeRegularExpression a samm-c:RegularExpressionConstraint ;
   samm:preferredName "Country Code Regular Expression"@en ;
   samm:description "Regular Expression that ensures a three-letter code "@en ;
   samm:see <https://www.iso.org/iso-3166-country-codes.html> ;
   samm:value "^[A-Z][A-Z][A-Z]$" .

:HexCodeCharacteristic a samm:Characteristic ;
   samm:preferredName "Hex code characteristic"@en ;
   samm:description "Describing a hex code: 0-9, a-f, A-F, optional 0x"@en ;
   samm:dataType xsd:string .

:HexRegularExpression a samm-c:RegularExpressionConstraint ;
   samm:preferredName "Hex regular expression"@en ;
   samm:description "This regular expression enforces a hex string. It ensures 0-9, a-f, A-F with an optional prefix 0x.\nValid examples:\n- 0x0123456789abcdefABCDEF\n- 0123456789abcdefABCDEF\n"@en ;
   samm:value "^(0x)?[0-9a-fA-F]+$" .

:Float a samm:Characteristic ;
   samm:preferredName "Float"@en ;
   samm:description "Characteristic of a float value."@en ;
   samm:dataType xsd:float .

:ConstraintLatitude a samm-c:RangeConstraint ;
   samm-c:minValue "-90.0"^^xsd:float ;
   samm-c:maxValue "90.0"^^xsd:float ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:AT_MOST .

:ConstraintLongitude a samm-c:RangeConstraint ;
   samm-c:minValue "-180.0"^^xsd:float ;
   samm-c:maxValue "180.0"^^xsd:float ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:AT_MOST .

:SemanticModelExpression a samm-c:RegularExpressionConstraint ;
   samm:preferredName "Semantic model expression"@en ;
   samm:description "Semantic model expression. Starting from 1.0.0 to 999.999.999."@en ;
   samm:value "^[1-99].[0-999].[0-999]$" .

