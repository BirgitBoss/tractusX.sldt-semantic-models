xcludingBiogenic" ] [ samm:property :pcfIncludingBiogenicUptake; samm:payloadName "pcfIncludingBiogenic" ] [ samm:property :fossilGhgEmissions; samm:optional true; samm:payloadName "fossilGhgEmissions" ] [ samm:property :biogenicNonCO2Emissions; samm:optional true; samm:payloadName "biogenicCarbonEmissionsOtherThanCO2" ] [ samm:property :biogenicCarbonWithdrawal; samm:optional true; samm:payloadName "biogenicCarbonWithdrawal" ] [ samm:property :landUseChangeGhgEmissions; samm:optional true; samm:payloadName "dlucGhgEmissions" ] [ samm:property :aircraftGhgEmissions; samm:optional true; samm:payloadName "aircraftGhgEmissions" ] [ samm:property :packagingPcfExcludingBiogenicUptake; samm:optional true; samm:payloadName "extWBCSD_packagingGhgEmissions" ] [ samm:property :distributionStagePcfExcludingBiogenicUptake; samm:optional true; samm:payloadName "distributionStagePcfExcludingBiogenicUptake" ] [ samm:property :distributionStagePcfIncludingBiogenicUptake; samm:optional true; samm:payloadName "distributionStagePcfIncludingBiogenicUptake" ] [ samm:property :distributionStageFossilGhgEmissions; samm:optional true; samm:payloadName "distributionStageFossilGhgEmissions" ] [ samm:property :distributionStageBiogenicNonCO2Emissions; samm:optional true; samm:payloadName "distributionStageBiogenicNonCO2Emissions" ] [ samm:property :distributionStageBiogenicCarbonWithdrawal; samm:optional true; samm:payloadName "distributionStageBiogenicCarbonWithdrawal" ] [ samm:property :distributionStageLandUseChangeGhgEmissions; samm:optional true; samm:payloadName "extTFS_distributionStageLandUseChangeGhgEmissions" ] [ samm:property :carbonContentTotal; samm:optional true; samm:payloadName "carbonContentTotal" ] [ samm:property :fossilCarbonContent; samm:optional true; samm:payloadName "extWBCSD_fossilCarbonContent" ] [ samm:property :biogenicCO2Uptake; samm:optional true; samm:payloadName "carbonContentBiogenic" ] [ samm:property :distributionStageAircraftGhgEmissions; samm:optional true