@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.simulated_week_based_capacity_group:1.0.0#> .
@prefix ext-week_based_capacity_group: <urn:samm:io.catenax.week_based_capacity_group:3.0.1#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#> .

:SimulatedWeekBasedCapacityGroup a samm:Aspect ;
   samm:preferredName "Material Demand"@en ;
   samm:description "The requirements of a customer towards a specific supplier for a specific material. Each material demand is unique by its Customer, Supplier and Material Number."@en ;
   samm:properties ( :simulationId :simulatedWeekBasedCapacityGroupId :simulationStatus :simulationReadyDate :simulationLastChanged :listOfWeekBasedCapacityGroup ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:simulationId a samm:Property ;
   samm:preferredName "Material Demand ID"@en ;
   samm:description "The Material Demand ID uniquely identifies the material demand within the business relationship between a customer and its supplier."@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "0157ba42-d2a8-4e28-8565-7b07830c1110" .

:simulatedWeekBasedCapacityGroupId a samm:Property ;
   samm:preferredName "Demand Series"@en ;
   samm:description "The demands for a dedicated material in a given time period of a given demand rate, distinguished by their demand location and demand category."@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "0157ba42-d2a8-4e28-8565-7b07830c1110" .

:simulationStatus a samm:Property ;
   samm:preferredName "xxx"@en ;
   samm:description "xxx"@en ;
   samm:characteristic :SimulationStatusCharacteristic ;
   samm:exampleValue "ACCEPTED" .

:SimulationStatusCharacteristic a samm-c:Enumeration ;
   samm:preferredName "Simulation Status"@en ;
   samm:description "."@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "SUCCESS" "FAILURE" "ACCEPTED" "REJECTED" "ABORTED" ) .

:simulationReadyDate a samm:Property ;
   samm:preferredName "Demand Series"@en ;
   samm:description "The demands for a dedicated material in a given time period of a given demand rate, distinguished by their demand location and demand category."@en ;
   samm:characteristic samm-c:Timestamp ;
   samm:exampleValue "2018-02-03T00:00:00"^^xsd:dateTime .

:simulationLastChanged a samm:Property ;
   samm:preferredName "Demand Series"@en ;
   samm:description "The demands for a dedicated material in a given time period of a given demand rate, distinguished by their demand location and demand category."@en ;
   samm:characteristic samm-c:Timestamp ;
   samm:exampleValue "2018-02-03T00:00:00"^^xsd:dateTime .

:listOfWeekBasedCapacityGroup a samm:Property ;
   samm:preferredName "Positions"@en ;
   samm:description "The positions refer to positions within an order that refer to the given product (see materialNumberSupplier, materialNumberCustomer and materialNumberDataSpace) for which the stocks are transferred. The positions are differentiated by the orderPositionReference."@en ;
   samm:characteristic :ListOfWeekBasedCapacityGroupCollection .

:WeekBasedCapacity a samm:Entity ;
   samm:preferredName "WeekBasedCapacity"@en ;
   samm:description "xxx"@en ;
   samm:properties ( 
      ext-week_based_capacity_group:capacityGroupId 
      ext-week_based_capacity_group:name
      ext-week_based_capacity_group:supplierLocations
      ext-week_based_capacity_group:supplier
      ext-week_based_capacity_group:customer
      ext-week_based_capacity_group:unitOfMeasure
      ext-week_based_capacity_group:linkedDemandSeries
      ext-week_based_capacity_group:capacities
      ext-week_based_capacity_group:changedAt
      ext-week_based_capacity_group:linkedCapacityGroups 
      ext-week_based_capacity_group:unitOfMeasureIsOmitted 
      ext-week_based_capacity_group:demandVolatilityParameters 
      ext-week_based_capacity_group:capacityGroupIsInactive 
       ) .

:ListOfWeekBasedCapacityGroupCollection a samm-c:Collection ;
   samm:preferredName "Positions Collection"@en ;
   samm:description "If any stock is available, then there is at least one position. If the supplier does differentiate the stock according to the order positions (an OrderPositionReference is set), then multiple positions can be given. There could also be only one position without an OrderPositionReference indicating an anonymous stock which is allocated to the customer."@en ;
   samm:dataType :WeekBasedCapacity .